<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wzz.mapper.BlogMapper">
    <select id="findAll" resultType="Blog">
        SELECT * FROM blog_list
    </select>
    <insert id="createBlog" parameterType="Blog">
        insert into blog_list(id,title,content,create_time,user_name)values (#{id},#{title},#{content},#{create_time},#{user_name})
    </insert>
</mapper>